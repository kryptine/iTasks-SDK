Ext.define('itwc.component.edit.Time',{
	alias: 'widget.itwc_edit_time',
	extend: 'Ext.form.field.Time',
	mixins: ['itwc.component.edit.Editable'],
	format: 'H:i:s',
	disableKeyFilter: true,
	queryCaching: false,
	initComponent: function() {
		this.callParent(arguments);
		this.initEditable();
	},
	getEditorValue: function() {
		return this.getRawValue();
	},
	validate: function() {
		return true;
	},
	postBlur: function() {
		//Don't update the raw value based on the parsed value
		//Leave that stuff up to the event handlers
		Ext.AbstractComponent.prototype.postBlur.apply(this,arguments);
	}
});
